# DO NOT EDIT THIS FILE

SUBMIT  := src/*.java test/*.java PLAN.txt

EXCLUDE := src/Broadcast.java src/CommandParser.java src/ServerBackend.java src/ServerError.java src/ServerMain.java src/ServerModelApi.java test/ChannelsMessagesTest.java test/ConnectionNicknamesTest.java test/InviteOnlyTest.java

JAVAC	:= javac

DEPENDS := src/*.java

TESTS := \
	test/ChannelsMessagesTest.java \
	test/ConnectionNicknamesTest.java \
	test/InviteOnlyTest.java \
	test/ServerModelTest.java


HWNAME := hw07
ts := $(shell /bin/date "+%Y-%m-%d-%H:%M:%S")

ZIPNAME := $(HWNAME)-submit($(ts)).zip

.PHONY: all Server clean zip

all:	

bin:
	mkdir bin

Server : $(DEPENDS) bin
	$(JAVAC) -d bin $(DEPENDS)

run% : bin/ServerMain.class
	cd bin && java ServerMain

zip: $(SUBMIT)
	zip '$(ZIPNAME)' $(SUBMIT) -x $(EXCLUDE)

clean:
	rm -f src/*.class bin/* test/*.class
	rm -rf *.zip
